{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ],
    "rules": {
        "no-any": true,
        "curly": true,
        "quotemark": [true, "single"],
        "prettier": true
    }
}